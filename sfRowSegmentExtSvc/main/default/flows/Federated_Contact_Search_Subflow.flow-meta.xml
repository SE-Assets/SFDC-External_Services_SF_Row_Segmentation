                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Search_SF_For_Phone</targetReference>
            </connector>
            <label>Valid Phone, No Email</label>
        </rules>
    </decisions>
