        <locationY>52</locationY>
        <defaultConnectorLabel>It&#39;s All Broke</defaultConnectorLabel>
        <rules>
            <name>Valid_Email</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varEmailAddress</leftValueReference>
                <operator>IsNull</operator>
